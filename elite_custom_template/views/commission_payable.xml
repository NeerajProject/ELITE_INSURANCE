<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="commission_payable_form_view" model="ir.ui.view">
            <field name="name">elite_custom_template.commission.payable.form</field>
            <field name="model">commission.payable</field>
            <field name="arch" type="xml">
                <form string="ModelTitle">
                    <header>
                                            <field name="status" widget="statusbar" readonly="1"/>
                        

                    </header>
                    <sheet>
                        <div class="oe_title">
                            <label for="name"/>
                            <h1>
                                <field name="name" placeholder="Title"/>
                            </h1>
                        </div>
                          <group col="3" class="mb16" >
                            <group>
                                <field name="branch_id"  readonly="status != 'draft'"></field>
<!--                                <field name="premium_paid_amounts"  readonly="status != 'draft'"></field>-->
                                <field name="date_from"  readonly="status != 'draft'"></field>

                            </group>
                            <group>
                                <field name="insurance_company"  readonly="status != 'draft'"></field>
<!--                                <field name="policy_status"  readonly="status != 'draft'"></field>-->
                                <field name="date_to"  readonly="status != 'draft'"></field>
                            </group>
                            <group>
                                <field name="partner_id"  readonly="status != 'draft'"></field>
                                <field name="user_id"  readonly="status != 'draft'"></field>
                                <field name="policy_type"  readonly="status != 'draft'"></field>
                                <button name="action_filters" type="object" string="Filters"  invisible="status != 'draft'"></button>
                            </group>
                        </group>

                             <notebook>
                            <page name="commission_payable" string="Premium Payments">
                                <field name="commission_payable_line">
                                    <tree create="0" edit="1" editable="top" delete="0">
<!--                                        <field name="project_id"></field>-->
                                        <field name="insurance_company_id" readonly="1"></field>
<!--                                        <field name="task_id"></field>-->
<field name="policy_sequence"  readonly="1"></field>
<field name="schedule_no" readonly="1"></field>
<field name="type_of_policy" readonly="1"></field>
<field name="customer_name_id" readonly="1"></field>
<field name="customer_name_id" readonly="1"></field>
<field name="producer_name_id" readonly="1"></field>
<field name="total_premium" readonly="1"></field>
<field name="total_premium_paid" readonly="1"></field>
<field name="total_premium_outstanding" readonly="1"></field>
<field name="elite_commission" readonly="1"></field>
<field name="elite_commission_amount" readonly="1"></field>
<field name="elite_commission_received" readonly="1"></field>
<field name="commission_outstanding" readonly="1"></field>
<field name="producer_commission_rate" readonly="1"></field>
<field name="producer_commission" readonly="1"></field>
<field name="producer_commission_paid" readonly="1"></field>
<field name="outstanding_producer_commission" readonly="1"></field>
<field name="select"></field>

                                    </tree>
                                </field>
                            </page>
                             </notebook>

                    </sheet>
                </form>
            </field>
        </record>

        <record id="commission_payable_tree_view" model="ir.ui.view">
            <field name="name">elite_custom_template.commission.payable.tree</field>
            <field name="model">commission.payable</field>
            <field name="arch" type="xml">
                <tree string="ModelTitle">

                <field name="name"></field>


                                 </tree>
            </field>
        </record>


        <record id="commission_payable_act_window" model="ir.actions.act_window">
            <field name="name">Commission Payable</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">commission.payable</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    There is no examples click here to add new ModelTitle.
                </p>
            </field>
        </record>

        <menuitem name="Commission Payable" id="commission_payable_menu"  action="commission_payable_act_window"/>

    </data>
</odoo>